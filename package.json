{
    "name": "zenstack-todo-sample-nextjs-tanstack",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run generate && npm run lint && next build",
        "start": "next start",
        "lint": "next lint",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate deploy",
        "db:reset": "prisma migrate reset",
        "db:browse": "prisma studio",
        "generate": "zenstack generate",
        "vercel-build": "npm run build && npm run db:deploy",
        "package-clean": "npm rm zenstack @zenstackhq/runtime @zenstackhq/server @zenstackhq/tanstack-query",
        "up": "npm run package-clean && npm install -D zenstack@next && npm install @zenstackhq/runtime@next @zenstackhq/server@next @zenstackhq/tanstack-query@next",
        "up-preview": "npm run package-clean && npm install --registry https://preview.registry.zenstack.dev -D zenstack@next && npm install --registry https://preview.registry.zenstack.dev @zenstackhq/runtime@next @zenstackhq/server@next @zenstackhq/tanstack-query@next"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^1.5.2",
        "@heroicons/react": "^2.0.12",
        "@neondatabase/serverless": "^0.9.0",
        "@prisma/adapter-neon": "^5.12.1",
        "@prisma/client": "^5.12.1",
        "@tanstack/react-query": "^5.8.4",
        "@tanstack/react-query-devtools": "^5.8.3",
        "@zenstackhq/runtime": "next",
        "@zenstackhq/server": "next",
        "@zenstackhq/tanstack-query": "next",
        "babel-plugin-superjson-next": "^0.4.5",
        "bcryptjs": "^2.4.3",
        "daisyui": "^4.0.7",
        "decimal.js": "^10.4.3",
        "moment": "^2.29.4",
        "nanoid": "^4.0.0",
        "next": "^14.1.4",
        "next-auth": "^5.0.0-beta.16",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-toastify": "^9.0.8",
        "superjson": "^1.12.0"
    },
    "devDependencies": {
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^14.17.3",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "autoprefixer": "^10.4.12",
        "eslint": "^7.32.0",
        "eslint-config-next": "12.3.1",
        "lower-case-first": "^2.0.2",
        "postcss": "^8.4.16",
        "prisma": "^5.12.1",
        "tailwindcss": "^3.1.8",
        "typescript": "^5.3.2",
        "zenstack": "next"
    },
    "browser": {
        "crypto": false
    }
